{
  "name": "comparateur_fournisseur_electricite",
  "version": "0.2.4",
  "private": true,
  "scripts": {
    "build": "REACT_APP_VERSION=$(node -p \"require('./package.json').version\") react-scripts build",
    "deadcode:all": "ts-prune",
    "deadcode:ci": "ts-prune | (! grep -v 'used in module') | (! grep -v 'migration') | (! grep -v 'd.ts')",
    "depcheck": "depcheck --ignores=node-fetch,@types/jest,sort-package-json",
    "depcheck:ci": "depcheck --ignores=node-fetch,@types/jest,sort-package-json | (! grep -v 'No depcheck issue') ",
    "lint": "eslint --ext .js,.ts .",
    "lint:ci": "eslint --ext .js,.ts . | (! grep -v 'warning') | (! grep -v 'error')",
    "sort-package:check": "sort-package-json --check",
    "sort-package:sort": "sort-package-json",
    "start": "react-scripts start",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testMatch '**/*.test.ts' --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@jonkoops/matomo-tracker": "^0.7.0",
    "@jonkoops/matomo-tracker-react": "^0.7.0",
    "@mui/icons-material": "^6.4.0",
    "@mui/material": "^6.4.0",
    "@mui/system": "^6.4.0",
    "@mui/x-charts": "^7.27.0",
    "@mui/x-date-pickers": "^7.24.1",
    "@mui/x-date-pickers-pro": "^7.27.3",
    "axios": "^1.8.2",
    "date-fns": "^4.1.0",
    "date-holidays": "^3.24.0",
    "eslint": "^8.0.0",
    "jszip": "^3.10.1",
    "notistack": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-router-dom": "^7.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.3",
    "@types/react-router-dom": "^5.3.3",
    "depcheck": "^1.4.7",
    "eslint-config-react-app": "^7.0.1",
    "sort-package-json": "^2.15.1",
    "ts-jest": "^29.2.5"
  }
}