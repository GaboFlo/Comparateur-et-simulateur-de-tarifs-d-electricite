{
  "name": "comparateur_fournisseur_electricite",
  "version": "dev",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:analyze": "vite build --config vite.config.analyze.ts",
    "build:serve": "vite build && npx serve build -s -l 3001",
    "deadcode:all": "ts-prune",
    "deadcode:ci": "ts-prune | (! grep -v 'used in module') | (! grep -v 'migration') | (! grep -v 'd.ts') | (! grep -v 'themePrimitives.ts') | (! grep -v 'vite.config.ts')",
    "depcheck": "depcheck --ignores=node-fetch,@types/jest,sort-package-json,ts-jest,typescript,@typescript-eslint/eslint-plugin",
    "depcheck:ci": "depcheck --ignores=node-fetch,@types/jest,sort-package-json,ts-jest,typescript,@typescript-eslint/eslint-plugin | (! grep -v 'No depcheck issue') ",
    "fetch-and-update-tempo-data": "cd services && tsx run-tempo.ts",
    "lint": "eslint --ext .js,.ts .",
    "lint:ci": "eslint --ext .js,.ts . --max-warnings 0",
    "preview": "vite preview",
    "sort-package:check": "sort-package-json --check",
    "sort-package:sort": "sort-package-json",
    "start": "vite",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --config jest.config.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@jonkoops/matomo-tracker": "^0.7.0",
    "@jonkoops/matomo-tracker-react": "^0.7.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.2.0",
    "@mui/system": "^7.3.1",
    "@mui/x-charts": "^7.29.1",
    "@mui/x-date-pickers": "^7.24.1",
    "@mui/x-date-pickers-pro": "^7.29.4",
    "axios": "^1.8.2",
    "date-holidays": "^3.24.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.23.12",
    "jszip": "^3.10.1",
    "notistack": "^3.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.5",
    "react-router-dom": "^7.6.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^5.0.2",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "jest": "^30.0.5",
    "rollup-plugin-visualizer": "^5.14.0",
    "sort-package-json": "^3.4.0",
    "terser": "^5.43.1",
    "ts-jest": "^29.3.1",
    "ts-prune": "^0.10.3",
    "tsx": "^4.20.3",
    "typescript": "^5.6.3",
    "vite": "^7.1.2",
    "vite-plugin-compression2": "^0.10.6",
    "vite-plugin-pwa": "^1.0.3"
  }
}