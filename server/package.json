{
    "name": "comparateur_fournisseur_electricite-server",
    "version": "1.0.0",
    "description": "",
    "license": "ISC",
    "author": "GaboFlo",
    "main": "src/index.ts",
    "scripts": {
        "deadcode:all": "ts-prune",
        "deadcode:ci": "ts-prune | (! grep -v 'used in module') | (! grep -v 'migration') | (! grep -v 'd.ts')",
        "depcheck": "depcheck --ignores=node-fetch,@types/jest,sort-package-json",
        "depcheck:ci": "depcheck --ignores=node-fetch,@types/jest,sort-package-json | (! grep -v 'No depcheck issue') ",
        "lint": "eslint --ext .js,.ts .",
        "lint:ci": "eslint --ext .js,.ts . | (! grep -v 'warning') | (! grep -v 'error')",
        "sort-package:check": "sort-package-json --check",
        "sort-package:sort": "sort-package-json",
        "start:dev": "nodemon --ext ts,js --exec ts-node ./src/index.ts",
        "start:prod": "ts-node ./src/index.ts",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testMatch '**/*.test.ts' --coverage"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "cors": "^2.8.5",
        "date-fns": "^4.1.0",
        "date-holidays": "^3.23.18",
        "express": "^4.21.2",
        "multer": "^1.4.5-lts.1",
        "node-cron": "^3.0.3",
        "unzipper": "^0.12.3",
        "uuid": "^11.0.5",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/multer": "^1.4.12",
        "@types/node-cron": "^3.0.11",
        "@types/unzipper": "^0.10.10",
        "depcheck": "^1.4.7",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "sort-package-json": "^2.5.1",
        "ts-node": "^10.9.1",
        "ts-prune": "^0.10.3"
    }
}